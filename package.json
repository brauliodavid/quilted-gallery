{
  "name": "quilted-gallery",
  "version": "0.0.6",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.js"
    }
  },
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs,iife --global-name QuiltedGallery --dts --sourcemap --minify && cp src/lib/style.css dist/style.css",
    "dev": "tsup src/index.ts --format esm,cjs,iife --global-name QuiltedGallery --dts --sourcemap --watch",
    "preview": "vite demo",
    "start": "vite demo"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.0",
    "@types/node": "^22.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brauliodavid/quilted-gallery"
  },
  "description": "Quilted gallery for images",
  "author": "Braulio David"
}
